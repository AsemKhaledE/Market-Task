@model Market.ViewModel.UserViewModel

<div class="col-md-6">
    <form asp-action="EditUser">
        <div class="form-group">
            <label asp-for="FullName" class="form-lable">FullName</label>
            <input asp-for="FullName" type="text" class="form-control " />
        </div>
        <div class="form-group">
            <label asp-for="UserName" class="form-lable">UserName</label>
            <input asp-for="UserName" type="text" class="form-control " />
        </div>
        <div class="form-group">
            <label asp-for="Password" class="form-lable">Password</label>
            <input asp-for="Password" type="text" class="form-control " />
        </div>
        <div class="form-group">
            <label asp-for="Email" class="form-lable">Email</label>
            <input asp-for="Email" type="text" class="form-control " />
        </div>
        <div class="form-group">
            <label asp-for="Phone" class="form-lable">Phone</label>
            <input asp-for="Phone" type="text" class="form-control " />
        </div>
        <p>
            <button type="submit" class="btn btn-primary"> Edit User  </button>
        </p>
    </form>

</div>
